{
    "name": "anafro/avacha",
    "version": "1.0.0",
    "description": "A supercharged web framework.",
    "minimum-stability": "stable",
    "license": "MIT",
    "authors": [
        {
            "name": "Anatoly Frolov (anafro)",
            "email": "anatolyfroloff@gmail.com"
        }
    ],
    "autoload": {
        "psr-4": {
            "App\\": "src/",
            "Avacha\\": "avacha/"
        },
        "files": [
            "avacha/Env/env.php",
            "avacha/Templating/templates.php",
            "avacha/Filesystem/paths.php",
            "avacha/Assets/assets.php",
            "avacha/Http/responses.php"
        ]
    },
    "require-dev": {
        "symfony/var-dumper": "^7.2"
    },
    "require": {
        "nikic/fast-route": "^1.3",
        "latte/latte": "^3.0",
        "vlucas/phpdotenv": "^5.6"
    }
}
